$(NAME)	Makefile	/^$(NAME): $(OBJ)$/;"	t
BHEADER	Makefile	/^BHEADER = get_next_line_bonus.h$/;"	m
BSRC	Makefile	/^BSRC = get_next_line_bonus.c get_next_line_utils_bonus.c$/;"	m
BUFFER_SIZE	get_next_line.h	/^#  define BUFFER_SIZE /;"	d
CFLAGS	Makefile	/^	CFLAGS = $(DFLAGS)$/;"	m
CFLAGS	Makefile	/^CFLAGS = -Wall -Wextra -Werror -g3 -O3 -I.$/;"	m
DFLAGS	Makefile	/^DFLAGS = -Wall -Wextra -Werror -g3 -I.$/;"	m
GET_NEXT_LINE_H	get_next_line.h	/^# define GET_NEXT_LINE_H$/;"	d
HEADER	Makefile	/^HEADER = get_next_line.h$/;"	m
NAME	Makefile	/^NAME = get_next_line$/;"	m
OBJ	Makefile	/^OBJ = $(SRC:.c=.o)$/;"	m
SFLAGS	Makefile	/^SFLAGS ?= -D BUFFER_SIZE=$(VALUE)$/;"	m
SRC	Makefile	/^SRC = .\/get_next_line.c .\/get_next_line_utils.c$/;"	m
VALUE	Makefile	/^VALUE ?= 0$/;"	m
all	Makefile	/^all: $(NAME)$/;"	t
bonus	Makefile	/^bonus:$/;"	t
buffer	get_next_line.h	/^	char			buffer[BUFFER_SIZE];$/;"	m	struct:s_file_info	typeref:typename:char[]
clean	Makefile	/^clean:$/;"	t
deb	Makefile	/^deb:$/;"	t
fclean	Makefile	/^fclean: clean$/;"	t
fd	get_next_line.h	/^	int				fd;$/;"	m	struct:s_file_info	typeref:typename:int
ft_build_line	get_next_line.c	/^char	*ft_build_line(t_file_info *set)$/;"	f	typeref:typename:char *
ft_read_line	get_next_line.c	/^char	*ft_read_line(t_file_info *info)$/;"	f	typeref:typename:char *
get_letter	get_next_line_utils.c	/^t_char	*get_letter(char letter)$/;"	f	typeref:typename:t_char *
get_next_line	get_next_line.c	/^char	*get_next_line(int fd)$/;"	f	typeref:typename:char *
len	get_next_line.h	/^	int				len;$/;"	m	struct:s_file_info	typeref:typename:int
link_letter	get_next_line_utils.c	/^void	link_letter(t_char **head, t_char *new_letter)$/;"	f	typeref:typename:void
main	get_next_line.c	/^int	main(int argc, char **argv)$/;"	f	typeref:typename:int
next	get_next_line.h	/^	t_char	*next;$/;"	m	struct:s_char	typeref:typename:t_char *
pos	get_next_line.h	/^	int				pos;$/;"	m	struct:s_file_info	typeref:typename:int
re	Makefile	/^re: clean all$/;"	t
read	get_next_line.h	/^	int				read;$/;"	m	struct:s_file_info	typeref:typename:int
s_char	get_next_line.h	/^struct s_char$/;"	s
s_file_info	get_next_line.h	/^typedef struct s_file_info$/;"	s
single_char	get_next_line.h	/^	char	single_char;$/;"	m	struct:s_char	typeref:typename:char
string	get_next_line.h	/^	t_char			*string;$/;"	m	struct:s_file_info	typeref:typename:t_char *
t_char	get_next_line.h	/^typedef struct s_char	t_char;$/;"	t	typeref:struct:s_char
t_file_info	get_next_line.h	/^}t_file_info;$/;"	t	typeref:struct:s_file_info
